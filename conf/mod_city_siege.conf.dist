###################################################################################################
# PLAYERBOT INTEGRATION
# NOTE: These settings only work if you have the mod-playerbots module installed!
#       If you don't have playerbots, these settings will be ignored (no errors).
###################################################################################################

#
#    CitySiege.Playerbots.Enabled
#        Description: Enable integration with mod-playerbots to recruit bots during sieges.
#                     Requires mod-playerbots module to be installed and compiled.
#        Default:     0 (disabled)

CitySiege.Playerbots.Enabled = 0

#
#    CitySiege.Playerbots.MinLevel
#        Description: Minimum level for playerbots to participate in city sieges
#        Default:     50

CitySiege.Playerbots.MinLevel = 50

#
#    CitySiege.Playerbots.MaxDefenders
#        Description: Maximum number of defending playerbots to teleport to city
#        Default:     30

CitySiege.Playerbots.MaxDefenders = 30

#
#    CitySiege.Playerbots.MaxAttackers
#        Description: Maximum number of attacking playerbots to teleport to spawn point
#        Default:     30

CitySiege.Playerbots.MaxAttackers = 30

#
#    CitySiege.Playerbots.RespawnDelay
#        Description: Time in seconds before a dead bot is resurrected and returned to battle
#                     Defenders respawn near the city leader (throne room), attackers respawn at spawn point
#        Default:     30

CitySiege.Playerbots.RespawnDelay = 30
